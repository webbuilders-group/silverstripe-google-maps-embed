{
    "name": "webbuilders-group/silverstripe-google-maps-embed",
    "description": "Adds support for embedding Google Maps in Silverstripe via oEmbed",
    "type": "silverstripe-vendormodule",
    "keywords": ["silverstripe", "google", "maps", "oembed"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Ed Chipman",
            "homepage": "https://webbuildersgroup.com",
            "role": "Developer"
        }
    ],
    "require": {
        "silverstripe/framework": "~4.11 | ^5",
        "embed/embed": "^4"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "~3.4"
    },
    "support": {
        "issues": "https://github.com/webbuilders-group/silverstripe-google-maps-embed/issues"
    },
    "autoload": {
        "psr-4": {
            "WebbuildersGroup\\GoogleMapsEmbed\\": "src/"
        }
    },
    "extra": {
        "expose": [
            "images"
        ]
    }
}
